<?xml version="1.0" encoding="utf-8"?>
<?xml-model href="PostSchema.xsd"?>
<Settings Version="1.0">
    <Description>
        <Comments></Comments>
        <CNCSystem></CNCSystem>
        <MachineName></MachineName>
        <Authors></Authors>
    </Description>
    <Common>
        <BreakArcs>No</BreakArcs> <!-- No, Cuts, Halfs, Quaters -->
        <BreakHelix>false</BreakHelix> <!-- false, true -->
        <ArcTolerance>0.01</ArcTolerance>
        <MaxCircleRadius>0</MaxCircleRadius>
        <DecimalSeparator>.</DecimalSeparator>
    </Common>
    <UserDefinedParameters>
        <Group ID="OutFiles" Caption="$(OutputFiles)">
            <Parameter ID="NCFileName" Caption="$(NCFileName)" Type="FileName"></Parameter>
            <Parameter ID="NCProgNumber" Caption="$(NCProgNumber)" Type="Integer">1</Parameter>
        </Group>
    </UserDefinedParameters>
</Settings>
