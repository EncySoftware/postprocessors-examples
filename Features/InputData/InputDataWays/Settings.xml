<?xml version="1.0" encoding="utf-8"?>

<?xml-model href="PostSchema.xsd"?>

<Settings Version="1.0">
    <Description>
        <Comments></Comments>
        <CNCSystem></CNCSystem>
        <MachineName></MachineName>
        <Authors></Authors>
    </Description>
    <Common>
        <!-- No, Cuts, Halfs, Quaters -->
        <BreakArcs>No</BreakArcs>
        <!-- false, true -->
        <BreakHelix>false</BreakHelix>
        <ArcTolerance>0.01</ArcTolerance>
        <MaxCircleRadius>0</MaxCircleRadius>
        <DecimalSeparator>.</DecimalSeparator>
    </Common>
    <UserDefinedParameters>
        <Group ID="OutFiles" Caption="$(OutputFiles)">
            <Parameter ID="NCFileName" Caption="$(NCFileName)" Type="FileName" />
            <Parameter ID="NCProgNumber" Caption="$(NCProgNumber)" Type="Integer">1</Parameter>
        </Group>
        <Group ID="Additional" Caption="Additional postprocessor options">
            <Parameter ID="ShowOptionsWindow" Caption="Show options window" Type="Boolean">false</Parameter>
            <Parameter ID="StopAtCloseWindow" Caption="Stop postprocessing after window close" Type="Boolean">true</Parameter>
        </Group>
    </UserDefinedParameters>
</Settings>
